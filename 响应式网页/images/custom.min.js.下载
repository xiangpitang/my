jQuery(function(e){setCustomHandlers(),setTimeout(function(){e(".woocommerce-message").remove(),e(".woocommerce-error").remove(),e(".woocommerce-info").remove()},2e3),e("#user_access_form").length&&(e("#user_access_form").modal({backdrop:"static",show:!1}),e("#user_access_form").find(".close_modal").click(function(){e("#user_access_form").modal("hide"),e("#user_access_form_form").removeClass("show-password-field"),e("#user_access_form_form").find(".use_password").show(),e("#user_access_form_form").find(".signin-link-holder").removeClass("col-sm-12"),e("#user_access_form_form").find(".signin-link-holder").addClass("col-sm-6"),e("#user_access_form_form").find("#send_signin_link").text("Send signin link")}),e("#user_access_form").find(".user_access_form_reset").click(function(){e("#user_access_form_form").removeClass("show-password-field"),e("#user_access_form_form").find(".use_password").show()})),e(".login_form").click(openLoginForm),e(".register_form").click(function(){openLoginForm(!0)}),e(".use_password").length&&(e(".use_password").click(function(){e("#user_access_form_form").addClass("show-password-field")}),e("#password").focus(function(){e("#user_access_form_form").hasClass("show-password-field")||e(this).blur()})),e("a.link-click").click(function(e){e.preventDefault()}),e("a.embed-click").click(function(e){e.preventDefault()}),new Clipboard(".link-click").on("success",function(e){alertMsg("Copied!")}),new Clipboard(".embed-click").on("success",function(e){alertMsg("Copied!")})});var setCustomHandlers=function(){var e=jQuery;e(".form-control").unbind("keyup"),e(".form-control").keyup(inputFloatLabel),e(".form-control").each(inputFloatLabel)},inputFloatLabel=function(){var e=jQuery,s=this,o=e(s).attr("placeholder")?e(s).attr("placeholder"):!1,o=e(s).attr("data-label")?e(s).attr("data-label"):o,r=(e(s).attr("type"),e(s).val());return o?r.length<=0?(e(s).hasClass("float-label")&&e(s).removeClass("float-label"),e(s).siblings(".float-label-text").remove(),!0):e(s).hasClass("float-label")?!0:(e('<label class="float-label-text">'+o+"</label>").insertBefore(s),void e(s).addClass("float-label")):!0},openLoginForm=function(e){e=e||!1,e===!0?($(".forgot_password").hide(),$(".sign-in-password").find(".btn").text("Sign up")):($(".forgot_password").show(),$(".sign-in-password").find(".btn").text("Sign in")),jQuery("#user_access_form").modal("show")},validateEmail=function(e){var s=/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;return e&&""!=e.trim()?s.test(e):!1},alertMsg=function(e){var s=jQuery;s("#alert_message").remove(),s("body").append('<div id="alert_message" class="text-center alert alert-warning alert-dismissible" role="alert">'+e+"</div>"),setTimeout(function(){s("#alert_message").remove()},2e3)},numberWithCommas=function(e){return e.toString().replace(/\B(?=(\d{3})+(?!\d))/g,",")};